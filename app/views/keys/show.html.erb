<h1>パスワード詳細</h1>

<div class='mb-5'>
  <table class='table table-hover'>
    <tbody>
      <tr>
        <th>ラベル</th>
        <td><%= @key.key_label %></td>
      </tr>
      <tr>
        <th>パスワード</th>
        <td><%= @key.key %></td>
      </tr>
      <tr>
        <th>作成日時</th>
        <td><%= @key.created_at %></td>
      </tr>
    </tbody>
  </table>
</div>
<div class='nav justify-content-end'>
  <%= link_to '編集', edit_key_path(@key), class: 'btn btn-primary' %>
</div>